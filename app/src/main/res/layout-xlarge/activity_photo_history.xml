    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_photo_history"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/colorGray"
    android:layout_height="match_parent"
    tools:context="com.app.easy_photo_to_link.PhotoHistoryActivity">

    <fragment
        android:id="@+id/fragmentHistory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        class="com.app.easy_photo_to_link.PhotoHistoryFragment"
        tools:layout="@layout/fragment_photohistory_list">

    </fragment>



</LinearLayout>
